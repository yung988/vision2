{
  "name": "studiovision-website",
  "version": "1.1.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "vercel-build": "pnpm install --no-frozen-lockfile && next build",
    "start": "next start",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "prepare": "[ -z \"$VERCEL\" ] && husky install || echo \"Skipping husky install on Vercel\"",
    "size": "npm run build && size-limit"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.19.0",
    "@ducanh2912/next-pwa": "^10.1.0",
    "@next/bundle-analyzer": "^14.0.4",
    "@radix-ui/react-accordion": "^1.1.2",
    "@size-limit/preset-app": "^11.0.1",
    "@studio-freight/compono": "^0.0.42",
    "@studio-freight/hamo": "^0.6.24",
    "@studio-freight/react-lenis": "0.0.38",
    "@studio-freight/tempus": "^0.0.38",
    "@svgr/webpack": "^8.1.0",
    "@theatre/core": "^0.7.0",
    "@theatre/studio": "^0.7.0",
    "@vercel/analytics": "^1.1.1",
    "blaze-slider": "1.9.3",
    "clsx": "^2.1.0",
    "contentful-migration": "^4.28.1",
    "critters": "^0.0.20",
    "cross-env": "^7.0.3",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "events": "^3.3.0",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6",
    "gsap": "^3.12.4",
    "idb-keyval": "^6.2.1",
    "jsonminify": "^0.4.2",
    "million": "2.6.4",
    "mime-types": "^2.1.35",
    "next": "14.0.4",
    "next-sitemap": "^4.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.49.2",
    "react-use": "^17.4.2",
    "react-use-measure": "^2.1.1",
    "sass": "^1.69.7",
    "throttle-debounce": "5.0.0",
    "webpack": "^5.98.0",
    "zustand": "4.4.7"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.8.1",
    "contentful-management": "^11.48.0",
    "dotenv": "^16.4.7",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "prettier-eslint": "^16.2.0",
    "size-limit": "^11.0.1",
    "stats.js": "^0.17.0"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,scss,md}": "prettier --write"
  },
  "pnpm": {
    "overrides": {
      "react-is": "18.2.0",
      "throttle-debounce": "5.0.0"
    }
  },
  "size-limit": [
    {
      "path": ".next/static/chunks/**/*.js"
    }
  ]
}
